{
	"name": "lazuli-frontend-starter",
	"version": "1.0.0",
	"description": "A frontend starter for lazuli projects",
	"main": "src/server.js",
	"repository": "https://github.com/Tyratox/lazuli-frontend",
	"author": "Tyratox (Nico Hauser)",
	"license": "UNLICENSED",
	"scripts": {
		"start": "npm run build && NODE_ENV=production node build/server.js",
		"dev": "npm run build && NODE_ENV=development node build/server.js",
		"relay": "relay-compiler --src ./src --schema ./src/schema.graphql",
		"build": "yarn build-frontend && yarn build-server",
		"build-frontend": "webpack --config webpack-build-frontend.config.js",
		"build-server": "webpack --config webpack-build-server.config.js"
	},
	"dependencies": {
		"compression": "^1.7.1",
		"express": "^4.15.5",
		"grid-styled": "^2.0.0-11",
		"history": "^4.7.2",
		"isomorphic-fetch": "^2.2.1",
		"isomorphic-relay": "^0.7.4",
		"prop-types": "^15.6.0",
		"react": "^16.0.0",
		"react-dom": "^16.0.0",
		"react-relay": "^1.4.1",
		"react-simple-maps": "^0.8.4",
		"relay-runtime": "^1.4.1",
		"request": "^2.83.0",
		"request-promise-native": "^1.0.5",
		"spdy": "^3.4.7",
		"styled-components": "^2.2.1",
		"universal-router": "^4.2.0"
	},
	"devDependencies": {
		"babel-core": "^6.26.0",
		"babel-loader": "^7.1.2",
		"babel-plugin-relay": "^1.4.1",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"babel-plugin-transform-object-rest-spread": "^6.26.0",
		"babel-preset-env": "^1.6.0",
		"babel-preset-react": "^6.24.1",
		"eslint": "^4.7.2",
		"eslint-plugin-ava": "^4.2.2",
		"eslint-plugin-babel": "^4.1.2",
		"eslint-plugin-react": "^7.4.0",
		"react-hot-loader": "^3.0.0-beta.7",
		"relay-compiler": "^1.4.1",
		"webpack": "^3.6.0",
		"webpack-dev-middleware": "^1.12.0",
		"webpack-hot-middleware": "^2.19.1",
		"webpack-node-externals": "^1.6.0"
	}
}
